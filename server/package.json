{
  "name": "show-case",
  "version": "1.0.0",
  "description": "some examples of using Strongly framework.",
  "main": "index.js",
  "scripts": {
    "start": "ts-node ./src/start.ts",
    "test": "jest --coverage"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "fastify": "^3.21.0",
    "fastify-cookie": "^4.1.1",
    "fastify-cors": "^5.1.0",
    "fastify-jwt": "^2.1.3",
    "mongodb": "^3.7.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.4.17",
    "rxjs": "^7.3.0",
    "socket.io": "^4.2.0",
    "strongly": "^1.0.75",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testdeck/jest": "^0.1.2",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.165",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^14.14.8",
    "@types/node-cache": "^4.2.5",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^27.2.1",
    "mongodb-memory-server": "^7.4.0",
    "prettier": "^1.19.1",
    "ts-essentials": "^7.0.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.(ts)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transformIgnorePatterns": [],
    "globals": {
      "ts-jest": {
        "compiler": "typescript",
        "tsconfig": "tsconfig.json"
      }
    },
    "detectOpenHandles": true
  }
}
